<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
<bean id="passportinfo" class="com.gcl.webimpl.ShenZhouTongBaoRequest">
	<property name="url" value="http://www.baidu.com"/>
	<property name="paraList">
	   <list>
	   	<bean class="org.apache.http.message.BasicNameValuePair">
		  	<constructor-arg index="0" value="name1"/>
	   		<constructor-arg index="1" value="value1"/>
	   	</bean>
	   </list>
	</property>
</bean>
<bean id="requestchain" class="com.gcl.http.HttpRequestChain">
<property name="requestList">
  <list>
   <ref bean="passportinfo" />
  </list>
</property>
</bean>
</beans>